    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Data\support_intents.csv" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Data\support_intents.csv">
